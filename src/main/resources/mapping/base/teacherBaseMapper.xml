                `name` = #{name},
            </if>
            <if test="intro != null and intro!=''">
                `intro` = #{intro},
            </if>
        </trim>
        WHERE
        <trim suffixOverrides="and">
                `teacher_id` = #{teacherId} and
        </trim>
    </update>

    <update id="update">
