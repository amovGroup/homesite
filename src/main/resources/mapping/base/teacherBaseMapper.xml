            <if test ="where.rightFuzzyIntro!=null and where.rightFuzzyIntro.size()>0">
                (
                <foreach collection="where.rightFuzzyIntro"  separator="or" item="item">
                    `Intro` like concat(#{item},'%')
                </foreach>
                ) and
            </if>
        </trim>
    </update>


